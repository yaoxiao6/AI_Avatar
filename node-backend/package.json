{
    "name": "rag-graphql-backend",
    "version": "1.0.0",
    "description": "GraphQL backend for RAG system",
    "main": "dist/index.js",
    "scripts": {
        "clean": "rm -rf ./dist",
        "build": "npm run clean && tsc",
        "start": "node dist/index.js",
        "dev": "nodemon --exec ts-node src/index.ts",
        "prestart": "npm run build",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "axios": "^1.6.7",
        "express": "^4.18.2",
        "form-data": "^4.0.0",
        "graphql": "^16.8.1",
        "graphql-http": "^1.22.0",
        "graphql-yoga": "^5.1.1",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "pg": "^8.11.3",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "@types/express": "^5.0.1",
        "@types/morgan": "^1.9.9",
        "@types/node": "^22.14.0",
        "@types/pg": "^8.11.11",
        "@types/winston": "^2.4.4",
        "nodemon": "^3.0.3",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3"
    }
}
